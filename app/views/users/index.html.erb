<%= render 'books/list', book: @book, user: current_user %>

<div class="col-md-8 offset-md-1">
        <h2>Users</h2>
        <table class="table">
          <thead>
           <tr>
            <th>image</th>
            <th>name</th>
            <th colspan="3"></th>
           </tr>
          </thead>
          <tbody>
           <tr><% @users.each do |user| %>
            <td><%= image_tag user.get_profile_image(100,100) %></td>
            <td>name <%= user.name %></td>
            <td><%= link_to "Show", user_path(user) %></td>
           </tr>
           <% end %>
          </tbody>
        </table>
      </div>

